{
  "functions": {
    "source": "functions",
    "gen": 2,
    "predeploy": [
      "npm --prefix \"$RESOURCE_DIR\" run build"
    ],
    "runtime": "nodejs20",
    "build": {
      "env": {
        "GOOGLE_CACHE_ENABLED": "false"
      }
    }
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  }
}
